generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
  credentials credentials[]

  @@map("Users")
}

model credentials {
  id  Int @id @default(autoincrement())
  url String 
  author users @relation(fields: [userName], references: [email])
  userName String
  password String
  title String
  
  @@map("Credentials")
}


