generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id          Int           @id @default(autoincrement())
  email       String        @unique
  password    String
  credentials credentials[]

  @@map("Users")
}

model credentials {
  id       Int    @id @default(autoincrement())
  url      String
  userId   Int
  userName String
  password String
  title    String
  user     users  @relation(fields: [userId], references: [id])

  @@map("Credentials")
}
